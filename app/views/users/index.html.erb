<div class="infoBox">
  <div class="infoBoxDiv">
    <h2 class="infoBoxTitle">Usuarios</h2>
      <%= form_tag list_path, :method => 'get', :id => "users_search" do %>
              <div class="center">
                <%= text_field_tag :search, params[:search] %>
                <%= submit_tag "Search", :name => nil %>
              </div>
        <div id="users"><%= render 'list', obj: @users%></div>
      <% end %>
  </div>
</div>